<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="Travel_Planner.NewWindow"
        Title="Nowe Okno"
        Width="400" Height="300">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        
        <TextBlock x:Name="statsImie" Text="statystkiImie" TextAlignment="Left"/>
        
        <TextBlock x:Name="statsNazwisko" Text="statystkiNazwisko" TextAlignment="Left" Margin="0,0,0,20"/>
        
        <TextBlock x:Name="statsCountryPicked" Text="statystkiKraj" TextAlignment="Left" Margin="0,0,0,20"/>
        
        <TextBlock x:Name="statsWybraneMiejsca" Text="statystykiWybraneMiejsce" TextAlignment="Left"/>
        
        <Button Content="Zamknij" Click="CloseButton_Click" HorizontalAlignment="Center" />
        
    </StackPanel>
</Window>